<%= f = form_for @conn, Routes.sheet_path(@conn, :create), [as: :sheet] %>
    <div class="form-row">
        <div class="form-group col-md-8">
            <%= select f, :book_id, @books, class: "form-control custom-select"%>
        </div>
        <div class="form-group col-md-4">
            <%= submit "Submit", class: "btn btn-primary" %>
        </div>
    </div>
</form>
